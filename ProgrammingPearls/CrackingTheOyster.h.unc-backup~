#ifndef ACRUSH_CRACKINGTHEOYSTER_H
#define ACRUSH_CRACKINGTHEOYSTER_H

#include "StandardLibrary.h"

using namespace std;

template <typename T>
inline void inspect(T& t) {
    typename T::iterator i1 = t.begin(), i2 = t.end();
    while (i1 != i2) {
        std::cout << (*i1) << ' ';
        i1++;
    }
    std::cout << std::endl;
}

// uses the Standard Library qsort to sort a file of integers
void problem_1_qsort(int array[], size_t size);

vector<int> problem_1_set(vector<int>& nums);

// bit vector

#endif //ACRUSH_CRACKINGTHEOYSTER_H
